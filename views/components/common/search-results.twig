<div class="search-result">
  {% if results %}
    <nav class="nav flex-column">
      {% for result in results %}
        {% set result = TimberPost( result.ID ) %}
        <a href="{{ result.id }}" class="nav-link">
          {{ result.name }}
        </a>
      {% endfor %}
    </ul>
  {% else %}
    <h5>No results found</h5>
  {% endif %}
</div>
